<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        ">
        
        
    <bean id="client" class="com.lovo.framestudy.beans.Client">
		<property name="name" value="张三"></property>
		<property name="address" value="抚琴东南路118号"></property>
		<property name="orderForm" ref="orderForm"></property>
	</bean>
	
	<bean id="orderForm" class="com.lovo.framestudy.beans.OrderForm">
		<property name="ofname" value="红烧牛肉面两碗"></property>
	</bean>
        
        
        
        
        
        
        
        
        
</beans>
        